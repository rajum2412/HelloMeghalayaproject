<?xml version="1.0" encoding="UTF-8"?>

<suite name="Suite" thread-count="5">

    <listeners>
         
         <listener class-name="HMTestNGproject.ExtentReportManager"/>
    </listeners>
       
  <test name="ChromeTest">
  <parameters>
      <parameter name="os" value="Windows"/>
